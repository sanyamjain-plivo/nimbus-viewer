{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "strict": true,
    "jsx": "react",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "outDir": "./dist",
    "types": ["jquery"]
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}